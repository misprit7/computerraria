{
  "configurations": {
    "Launch": {
      "adapter": "vscode-cpptools",
      "filetypes": [ "cpp", "c", "objc", "rust" ],
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/target/x86_64-unknown-linux-gnu/debug/template",
        "args": [],
        "cwd": ".",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "stopAtEntry": true
      }
    },
    "Attach": {
      "adapter": "vscode-cpptools",
      "filetypes": [ "cpp", "c", "objc", "rust" ],
      "configuration": {
        "request": "attach",
        "program": "${workspaceRoot}/target/x86_64-unknown-linux-gnu/debug/template",
        "MIMode": "gdb",
        "stopAtEntry": true
      }
    }
  }
}
